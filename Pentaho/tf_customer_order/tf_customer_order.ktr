    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <formula>
      <field_name>MarriedStatus</field_name>
      <formula_string>SUBSTITUTE(SUBSTITUTE([MaritalStatus];"M";"Married");"S";"Single")</formula_string>
      <value_type>String</value_type>
      <value_length>-1</value_length>
      <value_precision>-1</value_precision>
      <replace_field>MaritalStatus</replace_field>
    </formula>
    <formula>
      <field_name>Gender</field_name>
      <formula_string>SUBSTITUTE(SUBSTITUTE([Gender];"M";"Male");"F";"Female")</formula_string>
      <value_type>String</value_type>
      <value_length>-1</value_length>
      <value_precision>-1</value_precision>
      <replace_field>Gender</replace_field>
    </formula>
    <formula>
      <field_name>FullName</field_name>
      <formula_string>CONCATENATE(IF([Gender]="Male";"Mr.";IF([MaritalStatus]="Married";"Mrs.";"Miss."));[FirstName];" ";[LastName])</formula_string>
      <value_type>String</value_type>
      <value_length>-1</value_length>
      <value_precision>-1</value_precision>
      <replace_field/>
    </formula>
    <formula>
      <field_name>YearlyIncome</field_name>
      <formula_string>TRIM(SUBSTITUTE(SUBSTITUTE(SUBSTITUTE([YearlyIncome];"$";"");",";"");".00";""))</formula_string>
      <value_type>Integer</value_type>
      <value_length>-1</value_length>
